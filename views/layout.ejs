.first {
    background: rgba(255, 255, 255, 0.418);
    width: 45vw;
    overflow-x: scroll;
    overflow-y: scroll;
    height: 70vh;
    max-height: 90vh;
    white-space: pre;
    box-shadow: 0 1px 1px rgb(22, 22, 22);
    outline: none;
    padding: 15px;
    color: #000;
    font-family: 'Courier New', Courier, monospace;
    font-size: 14px;
    border-radius: 10px;
    box-shadow: inset 0 0 10px #fff, 0 0 5px #fff;
    transition: all .7s ease-out;
    transition-timing-function: ease-in-out;
    border-radius: 10px;
    border: solid 1px white;
    box-shadow: 0 0 10px white, inset 0 0 5px #000;
    position: absolute;
    left: 10vw;
    bottom: 3vh;
}

.second {
    background-color: rgba(177, 246, 248, 0.781);
    overflow-y: scroll;
    overflow-x: scroll;
    white-space: pre;
    box-shadow: 0 0 10px rgb(255, 255, 255), inset 0 0 10px black;
    border-radius: 10px;
    width: 40vw;
    height: 70vh;

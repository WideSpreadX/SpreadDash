/* TABLETS  */



@media only screen and (min-width: 600px) {


  h1 {
    font-family: 'Sulphur Point', sans-serif;
  }
.spread-menu {
  height: 100px;
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  border: none;
  box-shadow: none;
  outline: none;
}
.spread-search {
  width: 70%;
  height: 50px;
}
.spread-search input:first-of-type {
  background: rgba(45, 167, 207, 0.39);
  width: 80%;
  border-top-left-radius: 12px;
  border-bottom-left-radius: 12px;
  height: 24px;
  outline: none;
  box-shadow: inset 0 0 4px lightskyblue, 0 0 3px white;
  border: none;
  padding-left: 10px;
  margin-left: 30px;
  margin-top: 30px;
  color: white;
  font-family: 'Sulphur Point', sans-serif;
}
.spread-search input:last-of-type {
  border-radius: 50%;
  width: 50px;
  height: 50px;
  margin-left: -10px;
  font-family: 'Sulphur Point', sans-serif;
}
.spread-nav {
  width: 60px;
  height: 60px;
  position: fixed;
  top: 10px;
  right: 20px;
  border: none;
}
.spread-nav li {
  list-style: none;
  padding: 0;
}
.spread-nav img {
  width: 50px;
  height: 50px;
  box-shadow: 0 0 4px white, inset 0 0 4px lightskyblue, inset 0 0 8px rgba(255, 255, 255, 0.507), inset 0 0 12px rgba(255, 255, 255, 0.322);
  border-radius: 50%;
  transition: ease-out 0.4s;
  border: none;
}
.spread-nav img:hover {
  background:rgba(45, 167, 207, 0.205);
  box-shadow: inset 0 0 4px lightskyblue, inset 0 0 28px white, inset 0 0 36px lightskyblue, 0 0 4px white, 0 0 10px rgba(255, 255, 255, 0.459);
  border-bottom-left-radius: 4px;
  transition: ease-in-out 0.6s;
}
.spread-nav button {
  border-radius: 50%;
}
.message-button {
  display: none;
}

#spreadMenu {
  height: 60px;
  border-radius: 20px;
  outline: none;
  border: none;
  background: transparent;
  box-shadow: none;
  transition: ease-in 0.6s;
}
#spreadMenu:hover {
  border: none;
  transition: ease-in 0.6s;
}

.spread-menu-button {
}

/* SideNav */
.sidenav {
  height: 100%;
  width: 0;
  position: fixed;
  z-index: 1006;
  background: rgba(51, 51, 51, 0.671);
  left: 0;
  overflow-x: hidden;
  transition: 0.5s;
  padding-top: 60px;
  border-top-right-radius: 30px;
  border-left: groove 2px white;
  border-top: groove 1px white;
  box-shadow: inset 0 0 40px lightskyblue, 0 0 8px lightskyblue, 0 0 16px rgba(255, 255, 255, 0.795);
}
.sidenav:hover {
  background: rgba(0, 0, 0, 0.822);
  box-shadow: inset 0 0 30px white;
}
.sidenav a {
  padding: 8px 8px 8px 42px;
  text-decoration: none;
  font-size: 25px;
  color: rgba(0, 255, 255, 0.63);
  text-shadow: 0 0 3px white;
  display: block;
  transition: 0.6s;
}

.sidenav a:hover {
  transition: 0.3s;
  color: #f1f1f1;
}

.sidenav .closebtn {
  position: absolute;
  top: 15px;
  right: 15px;
  font-size: 35px;
  border-radius: 50%;
  text-align: center;
  width: 40px;
  color: rgba(255, 0, 0, 0.685);
  text-shadow: 0 0 2px rgb(138, 45, 45);
  height: 40px;
  padding: 0;
  border: none;
  font-weight: bolder;
  box-shadow: inset 0 0 10px rgba(255, 0, 0, 0.671), inset 0 0 20px white, 0 0 4px red;
}
.closebtn:hover {
  background: rgba(255, 0, 0, 0.705);
  color: white;
  box-shadow: inset 0 0 10px rgba(255, 0, 0, 0.671), inset 0 0 20px rgba(255, 255, 255, 0.541), 0 0 8px rgb(255, 0, 0);
}
}




/* Livestream Video */

#main-livestream-container {
  width: 100vw;
  height: 86vh;
  position: fixed;
  top: 12vh;
  left: 0;
  z-index: -1000;
  border-top-left-radius: 20px;
  border-top-right-radius: 20px;
}
#main-stream {
  background: rgba(255, 0, 0, 0.178);
  width: 95vw;
  height: 86vh;
  border-bottom-left-radius: 20px;
  border-bottom-right-radius: 20px;
  border-radius: 20px;
  margin-left: 2.5vw;

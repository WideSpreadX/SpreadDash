  cursor: help;
  padding: 3px;
  box-shadow: inset 0 0 10px rgba(255, 255, 255, 0.548), 0 0 3px rgba(255, 255, 255, 0.678), 0 0 4px lightskyblue, inset 0 0 4px lightskyblue;
}
#main-stream img {
  width: 120vw;
  margin-left: -25vw;
  margin-top: -25vh;
}
.ws-logo:hover {
  opacity: 0.7;
  transition: ease-in 0.4s;
  box-shadow: inset 0 0 6px rgba(255, 255, 255, 0.555), 0 0 8px white,  0 0 6px lightskyblue;
}
.ws-logo img {
  padding: 10px;
  width: 90px;
  height: 90px;
  opacity: 0.8;
  border-radius: 50%;
}

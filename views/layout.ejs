/* Graduation - Home */

.graduation-header {
  height: 110px;
  width: 100vw;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9999; 
  float: none;
}

.spreadUser-data-bar {
  width: 40%;
  height: 110px;
  float: left;
  background: rgba(233, 63, 63, 0.534);
  border-bottom-right-radius: 20px;
  border-right: groove 2px white;
  border-bottom: groove 2px white;
  box-shadow: 0 0 8px white, 0 0 5px red, inset 0 0 15px rgba(255, 255, 255, 0.685);
  transition: ease-in 0.5s;

}
.spreadUser-data-bar:hover {
  background: rgba(241, 107, 107, 0.507);
  transition: ease-in 0.5s;
}
#spreadUser-img {
  width: 93px;
  height: 93px;
  margin-top: 5px;
  margin-left: 10px;
  border-radius: 50%;
  border: 2px groove white;
  padding: 7px;
  cursor: pointer;
  box-shadow: 0 0 10px white, 0 0 5px red, inset 0 0 10px red, inset  0 0 3px white;
  float: left;
}
#spreadUser-img:hover {
  box-shadow: 0 0 10px white, 0 0 5px red, inset 0 0 10px red, inset  0 0 8px white, 0 0 8px white;
  background: rgba(255, 255, 255, 0.473);

}
#spreadUser-img img {
  width: 75px;
  height: 75px;
  border-radius: 50%;
  border: groove 1px white;
  box-shadow: 0 0 5px red, inset 0 0 8px black;

}
#spreadUser-data {
  width: 500px;
  height: 90px;
  float: left;
  position: absolute;
  left: 120px;
  top: 5px;
}
#spreadUser-name {
  width: 350px;
  height: 40px;
  line-height: 40px;
}
#spreadUser-name h3 {
  font-size: 32px;
  color: #fff;
  text-shadow: 0 0 2px #000;
  padding: 0;
  font-family: 'Julius Sans One', sans-serif;
  text-align: left;
}
#spreadUser-role {
  width: 150px;
  font-family: 'Julius Sans One', sans-serif;
  font-size: 22px;
}
#spreadUser-role h4 {
  color: rgba(163, 159, 159, 0.705);
  font-size: 24px;
  text-shadow: 0 0 2px rgba(102, 95, 95, 0.671), 0 0 1px red;
  font-weight: bolder;
}
#spreadUser-utils {
  width: 650px;
  float: none;
}
